#!/usr/bin/env python3

from argparse import ArgumentParser

import rawlib


def main():
    parser = ArgumentParser()
    parser.add_argument('raw_files', metavar='RAWFILE', nargs='*',
                        help='Raw files to process')
    args = parser.parse_args()

    for nef in args.raw_files:
        rawlib.miniature(nef)

if __name__ == '__main__':
    main()
